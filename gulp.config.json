{
  "paths": {
    "lessWatch": "src/**/*.less",
    "src": {
      "vendorJs": [
        "node_modules/angular/angular.min.js"
      ],
      "js": [
        "src/**/module.js",
        "src/**/*.js"
      ],
      "html": [
        "src/**/*.html"
      ],
      "media": [
        "src/media/**"
      ],
      "less": [
        "src/**/index.less"
      ],
      "index": "src/index.html"
    },
    "dist": {
      "index": ".",
      "js": "dist/js/",
      "css": "dist/css/",
      "media": "dist/media"
    }
  },
  "packageName": "overwatch-picker"
}
